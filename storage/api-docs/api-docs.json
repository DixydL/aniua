{
    "openapi": "3.0.0",
    "info": {
        "title": "Inari Api",
        "version": "1.0"
    },
    "paths": {
        "/api/anime": {
            "get": {
                "operationId": "App\\Http\\Controllers\\API\\AnimeController::index",
                "parameters": [
                    {
                        "name": "filter",
                        "in": "query",
                        "description": "filter",
                        "schema": {
                            "$ref": "#/components/schemas/AnimeFilterDataRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/AnimeData"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "operationId": "App\\Http\\Controllers\\API\\AnimeController::store",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AnimeData"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/anime/relative": {
            "get": {
                "operationId": "App\\Http\\Controllers\\API\\AnimeController::relative",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/AnimeData"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/anime/{id}": {
            "get": {
                "operationId": "App\\Http\\Controllers\\API\\AnimeController::show",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID anime",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AnimeData"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/genre": {
            "get": {
                "operationId": "App\\Http\\Controllers\\API\\GenreController::index",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenreData"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/user/register": {
            "post": {
                "tags": [
                    "User"
                ],
                "operationId": "App\\Http\\Controllers\\API\\RegisterController::register",
                "requestBody": {
                    "request": "UserArray",
                    "description": "List of user object",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterDataRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "AnimeData": {
                "properties": {
                    "id": {
                        "description": "Anime id",
                        "type": "number"
                    },
                    "name": {
                        "description": "Anime name",
                        "type": "string"
                    },
                    "name_origin": {
                        "description": "Anime name_origin",
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "country": {
                        "type": "string"
                    },
                    "episodes": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/EpisodeData"
                        }
                    },
                    "genres": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/GenreData"
                        }
                    },
                    "studio": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "current_episode": {
                        "type": "integer"
                    },
                    "count_episodes": {
                        "type": "integer"
                    },
                    "realese_date": {
                        "type": "integer"
                    },
                    "views": {
                        "type": "integer"
                    },
                    "season": {
                        "type": "integer"
                    }
                },
                "type": "object"
            },
            "AnimeDataRequest": {
                "properties": {
                    "id": {
                        "description": "Anime id",
                        "type": "number"
                    },
                    "name": {
                        "description": "Anime name",
                        "type": "string"
                    },
                    "name_origin": {
                        "description": "Anime name_origin",
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "country": {
                        "type": "string"
                    },
                    "studio": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "current_episode": {
                        "type": "integer"
                    },
                    "count_episodes": {
                        "type": "integer"
                    },
                    "realese_date": {
                        "type": "integer"
                    },
                    "views": {
                        "type": "integer"
                    },
                    "season": {
                        "type": "integer"
                    }
                },
                "type": "object"
            },
            "EpisodeData": {
                "properties": {
                    "id": {
                        "description": "Episode id",
                        "type": "number"
                    },
                    "name": {
                        "description": "Episode name",
                        "type": "string"
                    },
                    "episode": {
                        "type": "integer"
                    },
                    "iframe": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "GenreData": {
                "properties": {
                    "id": {
                        "description": "Anime id",
                        "type": "number"
                    },
                    "name": {
                        "description": "Anime name",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "AnimeFilterDataRequest": {
                "properties": {
                    "search": {
                        "type": "string"
                    },
                    "genres": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                },
                "type": "object"
            },
            "RegisterDataRequest": {
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    },
    "security": [
        []
    ]
}